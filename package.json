{
	"name": "sms-outreach-zone",
	"version": "0.1.1",
	"private": true,
	"scripts": {
		"debug": "NODE_OPTIONS=\"$NODE_OPTIONS --inspect\" next dev",
		"dev": "next dev",
		"build": "next build",
		"build:open-next": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' open-next build && node scripts/fix-opennext-pnpm-symlinks.mjs",
		"build:open-next:debug": "NODE_OPTIONS='--max-old-space-size=4096' OPEN_NEXT_DEBUG=true open-next build && node scripts/fix-opennext-pnpm-symlinks.mjs",
		"deploy:dev": "pnpm load-env && pnpm exec ./scripts/deploy-outreach.cjs",
		"start": "next start",
		"lint": "next lint --dir app --dir components --dir hooks --dir lib --dir types --dir utils",
		"lint-types": "tsc --noEmit --skipLibCheck",
		"lint:fix": "next lint --dir app --dir components --dir hooks --dir lib --dir types --dir utils --fix",
		"load-env": "set -o allexport; source .env; set +o allexport;",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:e2e": "playwright test",
		"prepare": "husky install"
	},
	"dependencies": {
		"@auth0/nextjs-auth0": "^4.9.0",
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-slot": "^1.2.4",
		"@tanstack/react-query": "^5.90.12",
		"axios": "^1.13.2",
		"bufferutil": "^4.0.9",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"date-fns": "^4.1.0",
		"flowbite": "^2.2.1",
		"flowbite-react": "^0.7.2",
		"frimousse": "^0.3.0",
		"jose": "^6.1.3",
		"lucide-react": "^0.309.0",
		"next": "14.2.25",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.68.0",
		"react-icons": "^5.5.0",
		"sonner": "^1.3.1",
		"styled-jsx": "5.1.1",
		"tailwind-merge": "^2.2.0",
		"twilio": "^5.10.6",
		"utf-8-validate": "^6.0.5",
		"worker-loader": "^3.0.8",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.4",
		"@commitlint/config-conventional": "^18.4.4",
		"@next/eslint-plugin-next": "^16.0.6",
		"@opennextjs/aws": "^3.6.6",
		"@playwright/test": "^1.41.0",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.6",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^8.48.1",
		"@typescript-eslint/parser": "^8.48.1",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-next": "14.1.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=18.17.0"
	}
}
